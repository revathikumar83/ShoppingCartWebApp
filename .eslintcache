User(user);\n        }else{\n            setUser(\"\");\n        }\n    })\n}\n\nuseEffect(()=>{\n    authListener();\n},[])\n\nconst{login,hidelogin}=props;\n\nreturn(\n    \n    <div>\n      { login && (   \n    <Modal isOpen={true} onRequestClose={props.hidelogin}>\n        <button className=\"closemodal\" onClick={props.hidelogin} >\n            x\n        </button>\n    <div>\n        { user ? (<Logout handleLogout={handleLogout}/>) :\n         (\n <Login email={email} password={password}\n setEmail={setEmail} setPassword={setPassword}\n emailerror={emailerror} passworderror={passworderror}\n hasAccount={hasAccount} sethasAccount={sethasAccount}\n handleLogin={handleLogin} handleSingup={handleSingup}\n />  \n        )\n        }\n     \n            \n    </div>\n    </Modal>\n      )}\n    </div>\n)\n}\n\nexport default Signup;","/Users/shivkumar/Documents/projects/shopping-cartapp/src/component/Login/Logout.js",[],"/Users/shivkumar/Documents/projects/shopping-cartapp/src/fire.js",[],{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","replacedBy":"129"},{"ruleId":"126","replacedBy":"130"},{"ruleId":"128","replacedBy":"131"},{"ruleId":"126","replacedBy":"132"},{"ruleId":"128","replacedBy":"133"},{"ruleId":"134","severity":1,"message":"135","line":2,"column":9,"nodeType":"136","messageId":"137","endLine":2,"endColumn":13},{"ruleId":"126","replacedBy":"138"},{"ruleId":"128","replacedBy":"139"},{"ruleId":"134","severity":1,"message":"140","line":11,"column":8,"nodeType":"136","messageId":"137","endLine":11,"endColumn":21},{"ruleId":"126","replacedBy":"141"},{"ruleId":"128","replacedBy":"142"},{"ruleId":"126","replacedBy":"143"},{"ruleId":"128","replacedBy":"144"},{"ruleId":"126","replacedBy":"145"},{"ruleId":"128","replacedBy":"146"},{"ruleId":"134","severity":1,"message":"147","line":14,"column":8,"nodeType":"136","messageId":"137","endLine":14,"endColumn":12},{"ruleId":"148","severity":1,"message":"149","line":31,"column":9,"nodeType":"150","messageId":"151","en